{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3AccessLimited",
      "Effect": "Allow",
      "Action": [
        "s3:PutObject",
        "s3:GetObject",
        "s3:ListBucket"
      ],
      "Resource": [
        "arn:aws:s3:::monty-checkpoints-*",
        "arn:aws:s3:::monty-checkpoints-*/*",
        "arn:aws:s3:::sim-artifacts-*",
        "arn:aws:s3:::sim-artifacts-*/*"
      ]
    }
  ]
}